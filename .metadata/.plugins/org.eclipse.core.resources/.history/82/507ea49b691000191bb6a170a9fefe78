package org.learn.java.general.programming;

public class Nthfibonacci {

	
		public static int fibo(int n) {
			if(n<=2) {
				return n;
			}
			else {
				return fibo(n - 1) + fibo(n - 2);
			}
			}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		fibo(5);
	}

}
